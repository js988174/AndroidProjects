<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white"
    android:nestedScrollingEnabled="false"
    android:fitsSystemWindows="true"
    tools:context=".Main.My39.Shop.viewPager.ShopStarFragment">

    <androidx.core.widget.NestedScrollView
        android:layout_width="match_parent"
        android:layout_height="0dp"
        android:fillViewport="true"
        app:layout_behavior="@string/appbar_scrolling_view_behavior"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <androidx.constraintlayout.widget.ConstraintLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">





    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout36"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:background="@color/colorGray"
        android:paddingLeft="44dp"
        android:paddingTop="15dp"
        android:paddingRight="44dp"
        android:paddingBottom="15dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent">

        <TextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendardsemi"
            android:text="실제 매장을 이용하신 고객님들이 남겨주신 별점입니다."
            android:textColor="@color/black2"
            android:textSize="14dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    </androidx.constraintlayout.widget.ConstraintLayout>


    <ImageView
        android:id="@+id/imageView56"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginLeft="20dp"
        android:layout_marginTop="30dp"
        android:src="@drawable/a_1"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout36" />

    <TextView
        android:id="@+id/kind_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_bold"
        android:text="직원분이 친절해요"
        android:textColor="@color/black2"
        android:textSize="14dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView56"
        app:layout_constraintStart_toEndOf="@+id/imageView56"
        app:layout_constraintTop_toTopOf="@+id/imageView56" />

    <ProgressBar
        android:id="@+id/kind_progress_bar"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginStart="11dp"
        android:background="@drawable/shop_progress_bar"
        android:max="100"
        android:progress="80"
        android:progressDrawable="@drawable/progress_bar"
        app:layout_constraintBottom_toBottomOf="@id/kind_rating"
        app:layout_constraintEnd_toStartOf="@+id/constraintLayout37"
        app:layout_constraintStart_toEndOf="@+id/kind_rating"
        app:layout_constraintTop_toTopOf="@+id/kind_rating" />


    <TextView
        android:id="@+id/kind_rating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lexend_deca_medium"
        android:text="4.9"
        android:textColor="@color/black2"
        android:textSize="14dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView56"
        app:layout_constraintStart_toStartOf="@+id/imageView56"
        app:layout_constraintTop_toBottomOf="@+id/imageView56" />


    <ImageView
        android:id="@+id/imageView57"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/a_2"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="@+id/kind_rating"
        app:layout_constraintStart_toStartOf="@+id/kind_rating"
        app:layout_constraintTop_toBottomOf="@+id/kind_rating" />

    <TextView
        android:id="@+id/time_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_bold"
        android:text="픽업시간을 잘 지켜줘요"
        android:textColor="@color/black2"
        android:textSize="14dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView57"
        app:layout_constraintStart_toEndOf="@+id/imageView57"
        app:layout_constraintTop_toTopOf="@+id/imageView57" />

    <ProgressBar
        android:id="@+id/pick_progress_bar"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginStart="11dp"
        android:background="@drawable/shop_progress_bar"
        android:max="100"
        android:progress="80"
        android:progressDrawable="@drawable/progress_bar"
        app:layout_constraintBottom_toBottomOf="@id/time_rating"
        app:layout_constraintEnd_toStartOf="@+id/constraintLayout37"
        app:layout_constraintStart_toEndOf="@+id/time_rating"
        app:layout_constraintTop_toTopOf="@+id/time_rating" />


    <TextView
        android:id="@+id/time_rating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lexend_deca_medium"
        android:text="4.9"
        android:textColor="@color/black2"
        android:textSize="14dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView57"
        app:layout_constraintStart_toStartOf="@+id/imageView57"
        app:layout_constraintTop_toBottomOf="@+id/imageView57" />


    <ImageView
        android:id="@+id/imageView58"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:src="@drawable/a_3"
        android:layout_marginTop="25dp"
        app:layout_constraintEnd_toEndOf="@+id/time_rating"
        app:layout_constraintStart_toStartOf="@+id/time_rating"
        app:layout_constraintTop_toBottomOf="@+id/time_rating" />

    <TextView
        android:id="@+id/clean_text"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/pretendard_bold"
        android:text="매장이 깨끗해요"
        android:textColor="@color/black2"
        android:textSize="14dp"
        android:layout_marginLeft="8dp"
        app:layout_constraintBottom_toBottomOf="@+id/imageView58"
        app:layout_constraintStart_toEndOf="@+id/imageView58"
        app:layout_constraintTop_toTopOf="@+id/imageView58" />


    <ProgressBar
        android:id="@+id/clean_progress_bar"
        style="@style/Widget.AppCompat.ProgressBar.Horizontal"
        android:layout_width="0dp"
        android:layout_height="3dp"
        android:layout_marginStart="11dp"
        android:background="@drawable/shop_progress_bar"
        android:max="100"
        android:progress="80"
        android:progressDrawable="@drawable/progress_bar"
        app:layout_constraintBottom_toBottomOf="@id/clean_rating"
        app:layout_constraintEnd_toStartOf="@+id/constraintLayout37"
        app:layout_constraintStart_toEndOf="@+id/clean_rating"
        app:layout_constraintTop_toTopOf="@+id/clean_rating" />

    <TextView
        android:id="@+id/clean_rating"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:fontFamily="@font/lexend_deca_medium"
        android:text="4.9"
        android:textColor="@color/black2"
        android:textSize="14dp"
        android:layout_marginTop="5dp"
        app:layout_constraintEnd_toEndOf="@+id/imageView58"
        app:layout_constraintStart_toStartOf="@+id/imageView58"
        app:layout_constraintTop_toBottomOf="@+id/imageView58" />


    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraintLayout37"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="60dp"
        android:layout_marginRight="64dp"
        android:paddingLeft="90dp"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraintLayout36">

        <TextView
            android:id="@+id/text1"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_bold"
            android:text="[ 참여"
            android:textColor="@color/black2"
            android:textSize="12dp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/member"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_bold"
            android:text="208"
            android:textColor="@color/black2"
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/text1"
            app:layout_constraintStart_toEndOf="@+id/text1"
            app:layout_constraintTop_toTopOf="@+id/text1" />

        <TextView
            android:id="@+id/text2"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:fontFamily="@font/pretendard_bold"
            android:text="명 ]"
            android:textColor="@color/black2"
            android:textSize="12dp"
            app:layout_constraintBottom_toBottomOf="@+id/member"
            app:layout_constraintStart_toEndOf="@+id/member"
            app:layout_constraintTop_toTopOf="@+id/member" />

        <ImageView
            android:id="@+id/imageView59"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="13dp"
            android:src="@drawable/a_4"
            app:layout_constraintEnd_toEndOf="@+id/text2"
            app:layout_constraintStart_toStartOf="@+id/text1"
            app:layout_constraintTop_toBottomOf="@+id/member" />

        <TextView
            android:id="@+id/sum"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:text="4.8"
            android:textSize="29dp"
            android:fontFamily="@font/lexend_bold"
            android:textColor="@color/black2"
            app:layout_constraintEnd_toEndOf="@+id/imageView59"
            app:layout_constraintStart_toStartOf="@+id/imageView59"
            app:layout_constraintTop_toBottomOf="@+id/imageView59" />




    </androidx.constraintlayout.widget.ConstraintLayout>



        </androidx.constraintlayout.widget.ConstraintLayout>

    </androidx.core.widget.NestedScrollView>
</androidx.constraintlayout.widget.ConstraintLayout>